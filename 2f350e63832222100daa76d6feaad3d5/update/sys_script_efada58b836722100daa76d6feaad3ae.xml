        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {
    var traceOutput = current.trace_output.toString();
    // gs.info('==== Raw trace output ===\n' + traceOutput);
    // Regex to extract IPv4 addresses
    var ipRegex = /\b\d{1,3}(?:\.\d{1,3}){3}\b/g;
    var matches = traceOutput.match(ipRegex);
    //gs.info('=== Regex matches===' + JSON.stringify(matches));
    if (matches && matches.length > 0) {
        var uniqueIPs = [];
        var seen = {};
        for (var i = 0; i < matches.length; i++) {
            var ip = matches[i];
            if (!seen[ip]) {
                seen[ip] = true;
                uniqueIPs.push(ip);
            }
        }
        //gs.info('===unique ips ===' + JSON.stringify(uniqueIPs));
        // Store as JSON list (or just comma separated string)
        current.ip_address = JSON.stringify(uniqueIPs, null, 2);

    }
})(current, previous);]]></script>
